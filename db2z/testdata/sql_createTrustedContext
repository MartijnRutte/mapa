CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( ADDRESS '127.0.0.1' )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( JOBNAME '*MVS1' )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( JOBNAME '*MVS1'
               , JOBNAME 'DB2PROD'
               )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    NO DEFAULT ROLE
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    DEFAULT ROLE ZZ9PZA WITHOUT ROLE AS OBJECT OWNER
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    DEFAULT ROLE ZZ9PZA WITH ROLE AS OBJECT OWNER AND QUALIFIER
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    DISABLE
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ENABLE
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    NO DEFAULT SECURITY LABEL
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    DEFAULT SECURITY LABEL AEIOU
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    WITH USE FOR IBMUSER
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    WITH USE FOR IBMUSER ROLE DBA
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    WITH USE FOR IBMUSER SECURITY LABEL XYZ
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    WITH USE FOR IBMUSER WITHOUT AUTHENTICATION
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    WITH USE FOR IBMUSER WITH AUTHENTICATION
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    WITH USE FOR IBMUSER ROLE DBA SECURITY LABEL XYZ WITHOUT AUTHENTICATION
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    WITH USE FOR IBMUSER ROLE DBA SECURITY LABEL XYZ WITH AUTHENTICATION
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    DEFAULT ROLE ZZ9PZA WITHOUT ROLE AS OBJECT OWNER
    ENABLE
    DEFAULT SECURITY LABEL AEIOU
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    WITH USE FOR IBMUSER ROLE DBA SECURITY LABEL XYZ WITH AUTHENTICATION
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    WITH USE FOR IBMUSER ROLE DBA SECURITY LABEL XYZ WITH AUTHENTICATION
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    DEFAULT SECURITY LABEL AEIOU
    ENABLE
    DEFAULT ROLE ZZ9PZA WITHOUT ROLE AS OBJECT OWNER
;

CREATE TRUSTED CONTEXT TC0001
    BASED UPON CONNECTION USING SYSTEM AUTHID ROOT
    WITH USE FOR IBMUSER WITH AUTHENTICATION SECURITY LABEL XYZ ROLE DBA
    ATTRIBUTES ( ADDRESS '127.0.0.1'
               , ENCRYPTION 'yesPlease'
               , SERVAUTH 'shibboleth'
               )
    DEFAULT SECURITY LABEL AEIOU
    ENABLE
    DEFAULT ROLE ZZ9PZA WITHOUT ROLE AS OBJECT OWNER
;


