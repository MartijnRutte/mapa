CREATE PROCEDURE TACHI.ROCINANTE.HOLDEN
  DYNAMIC RESULT SETS 4
  SELECT CURRENT DATE INTO V1 FROM SYSIBM.SYSDUMY1
;
!

--#SET TERMINATOR ;

PREPARE S1 FROM :V1;

COMMENT ON PUBLIC ALIAS FOO FOR TABLE IS '63C7DA60-0806-5858-522B-BE9447957951';

CREATE PROCEDURE TACHI.ROCINANTE.HOLDEN
  EXTERNAL NAME IEFBR14
  LANGUAGE COBOL
;

CREATE UNIQUE INDEX I1 ON T1(C1)        
  GENERATE KEY USING XMLPATTERN                               
  'declare default element namespace "http://example.com"; /t1c1/@c1ID'                                       
   AS SQL VARCHAR(4)
;


