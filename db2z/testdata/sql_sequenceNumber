UPDATE T1
      SET C1 = NEXT VALUE FOR SOME_SEQ
      WHERE C2 = 271828;

UPDATE T1
      SET C1 = PREVIOUS VALUE FOR SOME_SEQ
      WHERE C2 = 271828;

UPDATE T1
      SET C1 = NEXTVAL FOR SOME_SEQ
      WHERE C2 = 271828;

UPDATE T1
      SET C1 = PREVVAL FOR SOME_SEQ
      WHERE C2 = 271828;

UPDATE T1
      SET C1 = SOME_SEQ.NEXTVAL
      WHERE C2 = 271828;

UPDATE T1
      SET C1 = SOME_SEQ.CURRVAL
      WHERE C2 = 271828;


