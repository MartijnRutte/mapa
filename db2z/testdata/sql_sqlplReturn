
--#SET TERMINATOR @

CREATE FUNCTION CURR_TMST ()
  RETURNS TIMESTAMP
  LANGUAGE SQL
  CONTAINS SQL
  NO EXTERNAL ACTION
  BEGIN
  RETURN SELECT CURRENT TIMESTAMP FROM SYSIBM.SYSDUMMY1;
  END
@


CREATE FUNCTION SLOP (VAR INTEGER)
  RETURNS INTEGER
  LANGUAGE SQL
  NO EXTERNAL ACTION
  BEGIN
  IF VAR = 1 THEN GOTO LABEL1;
  ELSE RETURN 7;
  END IF;
  LABEL1: RETURN 44;
  END
@

