ALTER TRIGGER BRIDA REPLACE ACTIVE VERSION
  NO CASCADE BEFORE INSERT ON T1
  REFERENCING 
    OLD ROW AS OLD_ROW
    NEW ROW AS NEW_ROW
    OLD_TABLE AS OLD_TBL
    NEW_TABLE AS NEW_TBL
  FOR EACH ROW
  WHEN (NEW_ROW.C1 IS NOT NULL)
  BEGIN
    UPDATE T1
      SET NEW_ROW.LAST_UPDATE_TMST = CURRENT TIMESTAMP;
  END
@

ALTER TRIGGER AETHELFLAED REPLACE VERSION AEIOU
  NO CASCADE BEFORE INSERT ON T1
  REFERENCING 
    OLD ROW AS OLD_ROW
    NEW ROW AS NEW_ROW
    OLD_TABLE AS OLD_TBL
    NEW_TABLE AS NEW_TBL
  FOR EACH ROW
  WHEN (NEW_ROW.C1 IS NOT NULL)
  BEGIN
    UPDATE T1
      SET NEW_ROW.LAST_UPDATE_TMST = CURRENT TIMESTAMP;
  END
@


