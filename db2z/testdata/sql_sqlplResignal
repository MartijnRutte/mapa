CREATE PROCEDURE DIV05 (IN NUM INTEGER,
        IN DEN INTEGER,
        OUT RES INTEGER)
  LANGUAGE SQL
  CONTAINS SQL
  BEGIN
   DECLARE OVFL CONDITION for SQLSTATE '22003';
   DECLARE CONTINUE HANDLER FOR OVFL
     RESIGNAL SQLSTATE '271828' SET MESSAGE_TEXT = 'div by zero is bad';
   IF DEN = 0 THEN
     SIGNAL OVFL;
   ELSE
     SET RES = NUM / DEN;
   END IF;
  END
@

