
--#SET TERMINATOR @

CREATE PROCEDURE DIV01 (IN NUM INTEGER,
        IN DEN INTEGER,
        OUT RES INTEGER)
  LANGUAGE SQL
  CONTAINS SQL
  BEGIN
   DECLARE OVFL CONDITION for SQLSTATE '22003';
   DECLARE CONTINUE HANDLER FOR OVFL
     RESIGNAL SQLSTATE '271828';
   IF DEN = 0 THEN
     SIGNAL OVFL;
   ELSE
     SET RES = NUM / DEN;
   END IF;
  END
@

CREATE PROCEDURE DIV02 (IN NUM INTEGER,
        IN DEN INTEGER,
        OUT RES INTEGER)
  LANGUAGE SQL
  CONTAINS SQL
  BEGIN
   DECLARE OVFL CONDITION for SQLSTATE '22003';
   DECLARE CONTINUE HANDLER FOR OVFL
     RESIGNAL SQLSTATE VALUE '271828';
   IF DEN = 0 THEN
     SIGNAL OVFL;
   ELSE
     SET RES = NUM / DEN;
   END IF;
  END
@

