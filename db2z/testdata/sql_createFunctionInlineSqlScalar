CREATE FUNCTION BRAKEBILLS
  (
    DEANFOGG    INT AS LOCATOR
  ,             DECIMAL
  , WAUGH       FLOAT
  ,             REAL
  , ADIYODI     TABLE LIKE TRAVELER AS LOCATOR
  , WICKER      CHAR
  , HANSON      CHARACTER
  , QUINN       VARCHAR
  , ORLOFF_DIAZ VARGRAPHIC
  , COLDWATER   BINARY
  , FEN         VARBINARY
  , HOBERMAN    BINARY LARGE OBJECT (4M)
  , SCHIFF      BINARY VARYING (33)
  , LIPSON      TIMESTAMP
  , TODD        TIMESTAMP (6)
  , CHATWIN     TIMESTAMP WITHOUT TIME ZONE
  )
  RETURNS DATE
  LANGUAGE SQL
  SPECIFIC BRAKEBILLS
  PARAMETER CCSID ASCII
  PARAMETER CCSID EBCDIC
  PARAMETER CCSID UNICODE
  NOT DETERMINISTIC
  DETERMINISTIC
  EXTERNAL ACTION
  NO EXTERNAL ACTION
  READS SQL DATA
  CONTAINS SQL
  STATIC DISPATCH
  CALLED ON NULL INPUT
  NOT SECURED
  SECURED
  BEGIN
    DECLARE JULIA     CHAR;
    DECLARE QUENTIN   CHARACTER;
    DECLARE MARGO     CHARACTER VARYING (33);
    DECLARE ELLIOT    VARCHAR (42);
    DECLARE PENNY     INT;
    DECLARE KADY      INTEGER;
    DECLARE ALICE     DOUBLE PRECISION;
    
    DECLARE EXIT HANDLER FOR SQLEXCEPTION
    BEGIN
      GET DIAGNOSTICS AND OTHER IMPORTANT STUFF;
      DO SOME SORT OF LOGGING;
    END;
    
    ACTUAL_WORK: BEGIN
      SQL AND LOGIC WOULD GO HERE;
    END ACTUAL_WORK;
  END;

@


