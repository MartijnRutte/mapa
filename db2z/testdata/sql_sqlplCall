
--#SET TERMINATOR @

CREATE OR REPLACE TRIGGER TRIG001
  BEFORE INSERT ON T1
  REFERENCING NEW AS NEW
  FOR EACH ROW
  BEGIN
  LABEL001: CALL P1 (1);
  LABEL002: CALL P2 (1,2,3,4,5);
            CALL P3 (CURRENT DATE);
            CALL P4(1, CURRENT TIMESTAMP);
            CALL P5 ( NULL,NULL,NULL);
            CALL P6 (NULL, 4*3+2-1/5, CURRENT SQLID);
            CALL P7 (CHAR(6),NULL);
  END
@

