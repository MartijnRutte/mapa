   , :WS-VAR11 = MORE
   , :WS-VAR12 = NUMBER
   , :WS-VAR13 = ROW_COUNT
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = DB2_GET_DIAGNOSTICS_DIAGNOSTICS
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = DB2_SQL_NESTING_LEVEL
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     CONDITION :NB
     :WS-VAR01 = CATALOG_NAME
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     CONDITION :NB
     :WS-VAR01 = DB2_ORDINAL_TOKEN_1
   , :WS-VAR02 = DB2_ORDINAL_TOKEN_2
   , :WS-VAR03 = DB2_ORDINAL_TOKEN_3
   , :WS-VAR04 = DB2_ORDINAL_TOKEN_4
   , :WS-VAR05 = DB2_ORDINAL_TOKEN_5
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     CONDITION :NB
     :WS-VAR01 = CATALOG_NAME
   , :WS-VAR01 = CONDITION_NUMBER
   , :WS-VAR01 = CURSOR_NAME
   , :WS-VAR01 = DB2_ERROR_CODE1
   , :WS-VAR01 = DB2_ERROR_CODE2
   , :WS-VAR01 = DB2_ERROR_CODE3
   , :WS-VAR01 = DB2_ERROR_CODE4
   , :WS-VAR01 = DB2_INTERNAL_ERROR_POINTER
   , :WS-VAR01 = DB2_LINE_NUMBER
   , :WS-VAR01 = DB2_MESSAGE_ID
   , :WS-VAR01 = DB2_MODULE_DETECTING_ERROR
   , :WS-VAR64 = DB2_ORDINAL_TOKEN_100
   , :WS-VAR01 = DB2_REASON_CODE
   , :WS-VAR01 = DB2_RETURNED_SQLCODE
   , :WS-VAR01 = DB2_ROW_NUMBER
   , :WS-VAR01 = DB2_SQLERRD_SET
   , :WS-VAR01 = DB2_SQLERRD1
   , :WS-VAR01 = DB2_SQLERRD2
   , :WS-VAR01 = DB2_SQLERRD3
   , :WS-VAR01 = DB2_SQLERRD4
   , :WS-VAR01 = DB2_SQLERRD5
   , :WS-VAR01 = DB2_SQLERRD6
   , :WS-VAR01 = DB2_TOKEN_COUNT
   , :WS-VAR01 = RETURNED_SQLSTATE
   , :WS-VAR01 = SERVER_NAME
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     CONDITION :NB
     :WS-VAR01 = DB2_AUTHENTICATION_TYPE
   , :WS-VAR01 = DB2_AUTHORIZATION_ID
   , :WS-VAR01 = DB2_CONNECTION_STATE
   , :WS-VAR01 = DB2_CONNECTION_STATUS
   , :WS-VAR01 = DB2_ENCRYPTION_TYPE
   , :WS-VAR01 = DB2_SERVER_CLASS_NAME
   , :WS-VAR01 = DB2_PRODUCT_ID
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL STATEMENT
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL CONDITION
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL CONNECTION
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL CONDITION :DIAG-NB
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL CONNECTION :DIAG-NB
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL CONDITION 3
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL CONNECTION 3
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL STATEMENT, CONDITION, CONNECTION
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL STATEMENT
               , CONDITION :DIAG-NB
               , CONNECTION :DIAG-NB
END-EXEC

EXEC SQL GET STACKED DIAGNOSTICS
     :WS-VAR01 = ALL STATEMENT
               , CONDITION 3
               , CONNECTION 3
END-EXEC


