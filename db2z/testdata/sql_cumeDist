SELECT
    CUME_DIST(100, 200, 300) WITHIN GROUP (ORDER BY C1, C2, C3)
INTO :HV1, :HV2, :HV3
FROM L1.S1.T1 
WHERE C4 = 'XYZ'
;

SELECT
    CUME_DIST(100, 200, 300) 
    WITHIN GROUP
    (ORDER BY
         C1 ASC NULLS FIRST
       , C2 DESC NULLS LAST
       , C3 ASC NULLS LAST)
INTO :HV1, :HV2, :HV3
FROM L1.S1.T1 
WHERE C4 = 'XYZ'
;


