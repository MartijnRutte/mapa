EXEC SQL DESCRIBE CURSOR CSR1 INTO :SQLDA;
EXEC SQL DESCRIBE CURSOR :WS-CSR-NM INTO :SQLDA END-EXEC

EXEC SQL DESCRIBE INPUT STMT1 INTO :SQLDA END-EXEC

EXEC SQL DESCRIBE STMT2 INTO :SQLDA END-EXEC
EXEC SQL DESCRIBE STMT2 INTO :SQLDA USING NAMES END-EXEC
EXEC SQL DESCRIBE STMT2 INTO :SQLDA USING LABELS END-EXEC
EXEC SQL DESCRIBE STMT2 INTO :SQLDA USING ANY END-EXEC
EXEC SQL DESCRIBE STMT2 INTO :SQLDA USING BOTH END-EXEC
EXEC SQL DESCRIBE OUTPUT STMT2 INTO :SQLDA END-EXEC
EXEC SQL DESCRIBE OUTPUT STMT2 INTO :SQLDA USING NAMES END-EXEC
EXEC SQL DESCRIBE OUTPUT STMT2 INTO :SQLDA USING LABELS END-EXEC
EXEC SQL DESCRIBE OUTPUT STMT2 INTO :SQLDA USING ANY END-EXEC
EXEC SQL DESCRIBE OUTPUT STMT2 INTO :SQLDA USING BOTH END-EXEC

EXEC SQL DESCRIBE PROCEDURE PROCEDURE1 INTO :SQLDA END-EXEC
EXEC SQL DESCRIBE PROCEDURE :WS-PROCEDURE-NAME INTO :SQLDA END-EXEC

EXEC SQL DESCRIBE TABLE :WS-TBL INTO :SQLDA;
EXEC SQL DESCRIBE TABLE :WS-TBL INTO :SQLDA USING NAMES END-EXEC
EXEC SQL DESCRIBE TABLE :WS-TBL INTO :SQLDA USING LABELS END-EXEC
EXEC SQL DESCRIBE TABLE :WS-TBL INTO :SQLDA USING ANY END-EXEC
EXEC SQL DESCRIBE TABLE :WS-TBL INTO :SQLDA USING BOTH END-EXEC

