ALTER PROCEDURE DESTINY.YOUNG
ADD VERSION 2.0
     SELECT MAX(BONUS), MIN(BONUS), AVG(BONUS)
       INTO :WS-MAX, :WS-MIN, :WS-AVG
       FROM FARSCAPE.STUFF
       WITH UR
       QUERYNO 23
;
@

