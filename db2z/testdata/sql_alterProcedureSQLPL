ALTER PROCEDURE DESTINY
  ALTER ACTIVE VERSION
  NOT DETERMINISTIC
;

ALTER PROCEDURE DESTINY
  ALL VERSIONS DATE FORMAT ISO
;

ALTER PROCEDURE DESTINY
  VERSION ELI TIME FORMAT JIS
;

ALTER PROCEDURE DESTINY
  REPLACE ACTIVE VERSION
  (IN RUSH CHAR(4), OUT ELI VARCHAR(9))
  MODIFIES SQL DATA
  UPDATE SGU SET GENERAL = 'O''NEIL'
    WHERE GENERAL = 'HAMMOND'
;

