
--#SET TERMINATOR @

CREATE FUNCTION BRAKEBILLS ()
  RETURNS INT
  LANGUAGE COBOL
@

CREATE FUNCTION BRAKEBILLS ()
  RETURNS INT AS LOCATOR
  LANGUAGE COBOL
@

CREATE FUNCTION BRAKEBILLS ()
  RETURNS INT CAST FROM FLOAT
  LANGUAGE COBOL
@

CREATE FUNCTION BRAKEBILLS ()
  RETURNS INT CAST FROM DECIMAL(3) AS LOCATOR
  LANGUAGE COBOL
@

CREATE FUNCTION BRAKEBILLS (
    DEANFOG   INT AS LOCATOR
  , WICKER    DECIMAL
  , WAUGH     FLOAT
  , COLDWATER REAL
  , ADIYODI   TABLE LIKE TRAVELER AS LOCATOR
  )
  RETURNS INT CAST FROM DECIMAL(3) AS LOCATOR
  LANGUAGE COBOL
@


