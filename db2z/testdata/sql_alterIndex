   ALTER INDEX FARSCAPE.MOYA
     CLOSE NO;

   ALTER INDEX FARSCAPE.TALYN
     BUFFERPOOL BP1
     COPY YES
     PIECESIZE 8M;

   ALTER INDEX FARSCAPE.TALYN
     BUFFERPOOL BP1
     COPY YES
     PIECESIZE 8 M;

   ALTER INDEX X1
     NOT PADDED;

   ALTER INDEX FARSCAPE.MODULE
      BUFFERPOOL BP1
      CLOSE YES
      COPY YES
      USING VCAT VCAT01
      FREEPAGE 6
      PCTFREE 11
      GBPCACHE ALL
      ALTER PARTITION 3
         USING VCAT VCAT01
         FREEPAGE 13
         PCTFREE 13,
      ALTER PARTITION 4
         USING VCAT VCAT01
         GBPCACHE CHANGED,
      ALTER PARTITION 5
         USING VCAT VCAT01
         FREEPAGE 25
         PCTFREE 25;

ALTER INDEX FARSCAPE
ADD COLUMN(NORANTI RANDOM)
ADD COLUMN(JOOL RANDOM)
ADD INCLUDE COLUMN(SIKOZU)
