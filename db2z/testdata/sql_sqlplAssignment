
--#SET TERMINATOR @

CREATE OR REPLACE TRIGGER TRIG001
  BEFORE INSERT ON T1
  REFERENCING NEW AS NEW
  FOR EACH ROW
  BEGIN
  L001: SET A = 4;
  L002: SET B = NULL;
        SET C = DEFAULT;
        SET D[INDEX] = NULL;
  L003: SET E = POW(3.14159,2.71828);
        SET (F,G,H) = (NULL,42,DEFAULT);
        SET I['J'] = 'Jay';
        SET K = ARRAY[1,2,3];
        SET L = ARRAY[SELECT C1 FROM T1];
  END
@


