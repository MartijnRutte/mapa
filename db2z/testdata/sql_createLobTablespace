CREATE LOB TABLESPACE LTS1
;

CREATE LOB TABLESPACE LTS1
    IN DB1
;

CREATE LOB TABLESPACE LTS1
    BUFFERPOOL BP1
;

CREATE LOB TABLESPACE LTS1
    CLOSE YES
;

CREATE LOB TABLESPACE LTS1
    CLOSE NO
;

CREATE LOB TABLESPACE LTS1
    COMPRESS NO
;

CREATE LOB TABLESPACE LTS1
    COMPRESS YES
;

CREATE LOB TABLESPACE LTS1
    DEFINE YES
;

CREATE LOB TABLESPACE LTS1
    DEFINE NO
;

CREATE LOB TABLESPACE LTS1
    DSSIZE 4G
;

CREATE LOB TABLESPACE LTS1
    GBPCACHE CHANGED
;

CREATE LOB TABLESPACE LTS1
    GBPCACHE ALL
;

CREATE LOB TABLESPACE LTS1
    GBPCACHE SYSTEM
;

CREATE LOB TABLESPACE LTS1
    GBPCACHE NONE
;

CREATE LOB TABLESPACE LTS1
    LOCKMAX SYSTEM
;

CREATE LOB TABLESPACE LTS1
    LOCKMAX 108
;

CREATE LOB TABLESPACE LTS1
    LOCKSIZE ANY
;

CREATE LOB TABLESPACE LTS1
    LOCKSIZE LOB
;

CREATE LOB TABLESPACE LTS1
    LOGGED
;

CREATE LOB TABLESPACE LTS1
    NOT LOGGED
;

CREATE LOB TABLESPACE LTS1
    LOG YES
;

CREATE LOB TABLESPACE LTS1
    LOG NO
;

CREATE LOB TABLESPACE LTS1
    USING VCAT VCAT1
;

CREATE LOB TABLESPACE LTS1
    USING STOGROUP STG1
;

CREATE LOB TABLESPACE LTS1
    USING STOGROUP STG1 PRIQTY 1
;

CREATE LOB TABLESPACE LTS1
    USING STOGROUP STG1 SECQTY 2
;

CREATE LOB TABLESPACE LTS1
    USING STOGROUP STG1 ERASE NO
;

CREATE LOB TABLESPACE LTS1
    USING STOGROUP STG1 ERASE YES
;

CREATE LOB TABLESPACE LTS1
    IN DB1
    BUFFERPOOL BP1
    CLOSE YES
    COMPRESS YES
    DEFINE YES
    DSSIZE 4G
    GBPCACHE CHANGED
    GBPCACHE ALL
    GBPCACHE SYSTEM
    GBPCACHE NONE
    LOCKMAX SYSTEM
    LOCKSIZE ANY
    LOGGED
    LOG YES
    USING VCAT VCAT1
;

CREATE LOB TABLESPACE LTS1
    IN DB1
    BUFFERPOOL BP1
    CLOSE NO
    COMPRESS NO
    DEFINE NO
    DSSIZE 4G
    GBPCACHE CHANGED
    GBPCACHE ALL
    GBPCACHE SYSTEM
    GBPCACHE NONE
    LOCKMAX 108
    LOCKSIZE ANY
    NOT LOGGED
    LOG NO
    USING STOGROUP STG1
     PRIQTY 1
     SECQTY 2
     ERASE NO
;


