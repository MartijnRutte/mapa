CREATE OR REPLACE TRIGGER TRIG011
  BEFORE INSERT ON T1
  REFERENCING NEW AS NEW
  FOR EACH ROW
  WHEN (NEW.C1 IS NULL OR NEW.C1 > '20:30')
    P1: BEGIN
      CASE
        WHEN (NEW.C1 IS NULL) THEN
          CASE
            WHEN (NEW.C2 IS NULL) THEN
