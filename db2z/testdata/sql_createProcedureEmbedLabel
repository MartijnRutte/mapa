
--#SET TERMINATOR @

CREATE PROCEDURE TACHI.ROCINANTE.HOLDEN
  (IN numr INTEGER,
   IN deno INTEGER,
   OUT rslt INTEGER)
  DYNAMIC RESULT SETS 4
  BEGIN
    DECLARE FLAG CHAR(1) DEFAULT 'N';
    WHILE FLAG = 'N' DO
      GOTO MY_LABEL;
      SET FLAG = 'X';
      WHILE FLAG = 'N' DO
        SET RSLT = 0;
      END WHILE;
      MY_LABEL:
      UPDATE T1 SET C1 = FLAG;
      WHILE FLAG = 'N' DO
        SET FLAG = 'Y';
      END WHILE;
    END WHILE;

  END
;
@

CREATE PROCEDURE TACHI.ROCINANTE.HOLDEN
  (IN numr INTEGER,
   IN deno INTEGER,
   OUT rslt INTEGER)
  DYNAMIC RESULT SETS 4
  BEGIN
    DECLARE FLAG CHAR(1) DEFAULT 'N';

      UPDATE: UPDATE T1 SET C1 = FLAG;
      SQLERROR: DELETE FROM T1 WHERE C1 IS NULL;
      
  END
;
@


