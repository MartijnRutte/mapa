ALTER SPECIFIC FUNCTION MARX.FOCUS1 WLM ENVIRONMENT PRD001;

ALTER FUNCTION MARX.CENTER (CHAR(25), DEC(5,2), INTEGER)
      RETURNS NULL ON NULL INPUT;

ALTER FUNCTION MARX.CENTER (CHAR(), DEC(), INTEGER)
      RETURNS NULL ON NULL INPUT;

ALTER FUNCTION BRAKEBILLS.MAGIC (CLOB (4M) CCSID UNICODE)
      LANGUAGE COBOL PARAMETER STYLE SQL DETERMINISTIC
      CALLED ON NULL INPUT CONTAINS SQL NO EXTERNAL ACTION
      NO PACKAGE PATH SCRATCHPAD 7 NO FINAL CALL ALLOW PARALLEL
      NO DBINFO CARDINALITY 11 COLLID WICKER
      WLM ENVIRONMENT (PENNY,*) ASUTIME LIMIT 13
      STAY RESIDENT YES PROGRAM TYPE MAIN SECURITY DB2
      STOP AFTER SYSTEM DEFAULT FAILURES
      RUN OPTIONS 'ALL31(ON),HEAP(256K,256K,ANYWHERE,KEEP,8K,4K)'
      INHERIT SPECIAL REGISTERS STATIC DISPATCH SECURED
      ;

ALTER FUNCTION MY_UDF1 
		DETERMINISTIC;

ALTER FUNCTION GET_STUFF
	RESTRICT CARDINALITY 10000;


