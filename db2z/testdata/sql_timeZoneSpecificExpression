SELECT CURRENT TIMESTAMP AT LOCAL FROM SYSIBM.SYSDUMMY1;

SELECT CURRENT TIMESTAMP AT TIME ZONE '-06:00' FROM SYSIBM.SYSDUMMY1;

SELECT CURRENT TIMESTAMP WITH TIME ZONE FROM SYSIBM.SYSDUMMY1;

SELECT CURRENT TIMESTAMP WITHOUT TIME ZONE FROM SYSIBM.SYSDUMMY1;

SELECT CURRENT TIMESTAMP(8) AT LOCAL FROM SYSIBM.SYSDUMMY1;

SELECT CURRENT TIMESTAMP(8) AT TIME ZONE '-06:00' FROM SYSIBM.SYSDUMMY1;

SELECT CURRENT TIMESTAMP(8) WITH TIME ZONE FROM SYSIBM.SYSDUMMY1;

SELECT CURRENT TIMESTAMP(8) WITHOUT TIME ZONE FROM SYSIBM.SYSDUMMY1;

